{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "python/tts_engine.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/tts",
      "dest": "/python/tts_engine.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
